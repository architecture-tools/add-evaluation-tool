{
  "uid": "database-performance",
  "title": "Database Performance",
  "tags": [
    "database",
    "performance",
    "postgres"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 0,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "type": "timeseries",
      "title": "Database Operation Duration (p95)",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "histogram_quantile(0.95, sum(rate({__name__=~\"arch_eval_db_client_operation_duration_seconds_bucket|db_client_operation_duration_seconds_bucket\"}[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      },
      "note": "Note: Database metrics may not be available if not instrumented. This panel will show no data if metrics are missing."
    },
    {
      "id": 2,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "type": "timeseries",
      "title": "Database Operations per Second",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "sum(rate({__name__=~\"arch_eval_db_client_operation_duration_seconds_count|db_client_operation_duration_seconds_count\"}[5m]))",
          "legendFormat": "Ops/sec",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      },
      "note": "Note: Database metrics may not be available if not instrumented. This panel will show no data if metrics are missing."
    },
    {
      "id": 3,
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      },
      "type": "timeseries",
      "title": "Database Error Rate (%)",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "sum(rate({__name__=~\"arch_eval_db_client_operation_duration_seconds_count|db_client_operation_duration_seconds_count\", status=\"error\"}[5m])) / sum(rate({__name__=~\"arch_eval_db_client_operation_duration_seconds_count|db_client_operation_duration_seconds_count\"}[5m])) * 100",
          "legendFormat": "Error Rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "note": "Note: Database metrics may not be available if not instrumented. This panel will show no data if metrics are missing."
    }
  ]
}

