      "id": 1,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "type": "timeseries",
      "title": "Upload Success Rate (%)",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "(sum(rate({__name__=~\"arch_eval_http_server_request_duration_seconds_count|http_server_request_duration_seconds_count\", http_route=~\".*diagrams.*\", http_request_method=\"POST\", http_response_status_code=~\"2..\"}[5m])) / sum(rate({__name__=~\"arch_eval_http_server_request_duration_seconds_count|http_server_request_duration_seconds_count\", http_route=~\".*diagrams.*\", http_request_method=\"POST\"}[5m]))) * 100",
          "legendFormat": "Success Rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      }
    },
    {
      "id": 2,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
