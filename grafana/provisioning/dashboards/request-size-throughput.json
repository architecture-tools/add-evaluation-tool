{
  "uid": "request-size-throughput",
  "title": "Request Size & Throughput",
  "tags": [
    "throughput",
    "size",
    "performance"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 0,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "type": "timeseries",
      "title": "Request Body Size (p95)",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "histogram_quantile(0.95, sum(rate({__name__=~\"arch_eval_http_server_request_body_size_bytes_bucket|http_server_request_body_size_bytes_bucket\"}[5m])) by (le, http_route))",
          "legendFormat": "{{http_route}} - p95",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      }
    },
    {
      "id": 2,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "type": "timeseries",
      "title": "Response Body Size (p95)",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "histogram_quantile(0.95, sum(rate({__name__=~\"arch_eval_http_server_response_body_size_bytes_bucket|http_server_response_body_size_bytes_bucket\"}[5m])) by (le, http_route))",
          "legendFormat": "{{http_route}} - p95",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      }
    },
    {
      "id": 3,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "type": "timeseries",
      "title": "Average Request Size",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "sum(rate({__name__=~\"arch_eval_http_server_request_body_size_bytes_sum|http_server_request_body_size_bytes_sum\"}[5m])) by (http_route) / sum(rate({__name__=~\"arch_eval_http_server_request_body_size_bytes_count|http_server_request_body_size_bytes_count\"}[5m])) by (http_route)",
          "legendFormat": "{{http_route}}",
          "refId": "A"
