{
  "uid": "plantuml-parsing-performance",
  "title": "PlantUML Parsing Performance",
  "tags": [
    "parsing",
    "plantuml",
    "performance"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 0,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "type": "timeseries",
      "title": "Parsing p95 Latency",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "histogram_quantile(0.95, sum(rate({__name__=~\"arch_eval_http_server_request_duration_seconds_bucket|http_server_request_duration_seconds_bucket\", http_route=~\".*parse.*\"}[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 1,
                "color": "yellow"
              },
              {
                "value": 3,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "type": "timeseries",
      "title": "Parsing p99 Latency",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "histogram_quantile(0.99, sum(rate({__name__=~\"arch_eval_http_server_request_duration_seconds_bucket|http_server_request_duration_seconds_bucket\", http_route=~\".*parse.*\"}[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "id": 3,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "type": "timeseries",
      "title": "Parsing Success Rate (%)",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "(sum(rate({__name__=~\"arch_eval_http_server_request_duration_seconds_count|http_server_request_duration_seconds_count\", http_route=~\".*parse.*\", http_response_status_code=~\"2..\"}[5m])) / sum(rate({__name__=~\"arch_eval_http_server_request_duration_seconds_count|http_server_request_duration_seconds_count\", http_route=~\".*parse.*\"}[5m]))) * 100",
          "legendFormat": "Success Rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "red"
              },
              {
                "value": 90,
                "color": "yellow"
              },
              {
                "value": 95,
                "color": "green"
              }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "type": "timeseries",
      "title": "Parsing Requests per Second",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "sum(rate({__name__=~\"arch_eval_http_server_request_duration_seconds_count|http_server_request_duration_seconds_count\", http_route=~\".*parse.*\"}[5m]))",
          "legendFormat": "Parsing RPS",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      }
    },
    {
      "id": 5,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "type": "timeseries",
      "title": "Average Parsing Duration",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "sum(rate({__name__=~\"arch_eval_http_server_request_duration_seconds_sum|http_server_request_duration_seconds_sum\", http_route=~\".*parse.*\"}[5m])) / sum(rate({__name__=~\"arch_eval_http_server_request_duration_seconds_count|http_server_request_duration_seconds_count\", http_route=~\".*parse.*\"}[5m]))",
          "legendFormat": "Avg Duration",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    }
  ]
}

